{
	"containers": [{
		"image": "{{.DB_IMAGE}}",
		"name": "{{.DB_NAME}}-{{.HASH}}",
		"limits": {
			"cpu": {{.DB_CPU}},
			"memory": {{.DB_RAM}}
		},
		"env": [{
                "name": "MYSQL_ROOT_PASSWORD",
                "value": "{{.MYSQL_ROOT_PASSWORD}}"
              },
              {
                "name": "MYSQL_DATABASE",
                "value": "{{.MYSQL_DATABASE}}"
              },
              {
                "name": "MYSQL_USER",
                "value": "{{.MYSQL_USER}}"
              },
              {
                "name": "MYSQL_PASSWORD",
                "value": "{{.MYSQL_PASSWORD}}"
              }
	   ]
	}],
	"name": "{{.DB_NAME}}-{{.HASH}}",
	"replicas": 1
}
